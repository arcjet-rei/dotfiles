[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[alias]
	a = !git-alias
	ca = commit --all
	cav = commit --all --verbose
	cv = commit --verbose
	fa = fetch --all
	d = diff
	ds = diff --staged
	lg = log --oneline
	prom = pull --rebase origin main
	s = status -sb
